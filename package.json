{
    "name": "@bzafiris/madge",
    "version": "3.6.0",
    "author": "Patrik Henningsson <patrik.henningsson@gmail.com>",
    "repository": "git://github.com/pahen/madge",
    "homepage": "https://github.com/pahen/madge",
    "license": "MIT",
    "reveal": true,
    "description": "Create graphs from module dependencies.",
    "keywords": [
        "ES6",
        "ES7",
        "AMD",
        "RequireJS",
        "require",
        "module",
        "circular",
        "dependency",
        "dependencies",
        "graphviz",
        "graph"
    ],
    "engines": {
        "node": ">=10.x.x"
    },
    "main": "./lib/api",
    "bin": {
        "madge": "./bin/cli.js"
    },
    "scripts": {
        "test": "npm run lint && npm run mocha",
        "mocha": "mocha test/*.js",
        "watch": "mocha --watch --growl test/*.js",
        "lint": "eslint bin/cli.js lib test/*.js",
        "debug": "node bin/cli.js --debug bin lib",
        "generate": "npm run generate:small && npm run generate:madge",
        "generate:small": "bin/cli.js --image /tmp/simple.svg test/cjs/circular/a.js",
        "generate:madge": "bin/cli.js --image /tmp/madge.svg bin lib",
        "test:output": "./test/output.sh",
        "compile-libapijs": "babel lib\\api.js -o lib\\api.js --require babel-polyfill --presets env",
        "compile-liblogjs": "babel lib\\log.js -o lib\\log.js --require babel-polyfill --presets env",
        "compile-libtreejs": "babel lib\\tree.js -o lib\\tree.js --require babel-polyfill --presets env",
        "compile-libgraphjs": "babel lib\\graph.js -o lib\\graph.js --require babel-polyfill --presets env",
        "compile-libcyclicjs": "babel lib\\cyclic.js -o lib\\cyclic.js --require babel-polyfill --presets env",
        "compile-liboutputjs": "babel lib\\output.js -o lib\\output.js --require babel-polyfill --presets env",
        "compile-rollupconfigjs": "babel rollup.config.js -o rollup.config.js --require babel-polyfill --presets es2015-without-strict",
        "compile-1_script": "npm run compile-libapijs && npm run compile-liblogjs && npm run compile-libtreejs && npm run compile-libgraphjs && npm run compile-libcyclicjs && npm run compile-liboutputjs && npm run compile-rollupconfigjs",
        "compile": "npm run compile-1_script"
    },
    "funding": {
        "type": "opencollective",
        "url": "https://opencollective.com/madge"
    },
    "collective": {
        "type": "opencollective",
        "url": "https://opencollective.com/madge"
    },
    "dependencies": {
        "chalk": "^3.0.0",
        "commander": "^4.0.1",
        "commondir": "^1.0.1",
        "debug": "^4.0.1",
        "dependency-tree": "^7.0.2",
        "detective-amd": "^3.0.0",
        "detective-cjs": "^3.1.1",
        "detective-es6": "^2.1.0",
        "detective-less": "^1.0.2",
        "detective-postcss": "^3.0.1",
        "detective-sass": "^3.0.1",
        "detective-scss": "^2.0.1",
        "detective-stylus": "^1.0.0",
        "detective-typescript": "^5.7.0",
        "graphviz": "0.0.9",
        "ora": "^4.0.2",
        "pify": "^4.0.0",
        "pluralize": "^8.0.0",
        "pretty-ms": "^5.0.0",
        "rc": "^1.2.7",
        "walkdir": "^0.4.1"
    },
    "devDependencies": {
        "@aptoma/eslint-config": "^7.0.1",
        "eslint": "^6.6.0",
        "mocha": "^6.2.2",
        "mz": "^2.7.0",
        "should": "^13.2.3",
        "@rollup/plugin-commonjs": "^11.0.1",
        "@rollup/plugin-node-resolve": "^7.0.0",
        "babel-cli": "latest",
        "babel-core": "latest",
        "babel-plugin-add-module-exports": "latest",
        "babel-plugin-transform-es2015-modules-commonjs": "latest",
        "babel-polyfill": "latest",
        "babel-preset-env": "latest",
        "babel-preset-es2015-without-strict": "latest"
    }
}